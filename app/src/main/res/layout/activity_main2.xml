<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/layout1"
    tools:context=".MainActivity2">
    <androidx.constraintlayout.widget.Placeholder
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="150dp"
        android:id="@+id/place"/>
<ImageView
    android:id="@+id/acount"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/account"
    android:onClick="placeChange"
    app:layout_constraintBottom_toBottomOf="parent"
    android:layout_marginBottom="20dp"
    app:layout_constraintLeft_toLeftOf="parent"
    />
    <ImageView
        android:id="@+id/alarm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/alarm"
        android:onClick="placeChange"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toRightOf="@+id/acount"
        android:layout_marginLeft="5dp"/>
    <ImageView
        android:id="@+id/cal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/cal"
        android:onClick="placeChange"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toRightOf="@id/alarm"
        android:layout_marginLeft="5dp"/>
    <ImageView
        android:id="@+id/del"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/delete"
        android:onClick="placeChange"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toRightOf="@id/cal"
        android:layout_marginLeft="5dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/darkheart"
        android:onClick="placeChange"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toRightOf="@id/del"
        android:layout_marginLeft="5dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>